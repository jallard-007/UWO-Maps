<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane minWidth="250.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mapsJavaFX.NavigationController">
        <ButtonBar prefHeight="40.0" prefWidth="166.0" AnchorPane.leftAnchor="0.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#goToProfile" text="Profile" />
                <Button mnemonicParsing="false" onAction="#goToHelp" text="Help" />
            </buttons>
            <padding>
                <Insets left="-10.0" right="10.0" />
            </padding>
        </ButtonBar>
        <Button mnemonicParsing="false" onAction="#logout" text="Logout" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="7.0" />
</AnchorPane>
